@model IEnumerable<Musica_SucapucaRodrigo.Models.Album>

<p>
    <a class="btn btn-primary" asp-action="Create">Crear nuevo Artista</a>
</p>

<h1>Albumes</h1>
<table class="table table-hover">     
    <tbody>
        @foreach (var item in Model) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Title)
                    </td> 
                </tr>
        }
    </tbody>
</table>
